    <%= stylesheet_link_tag 'storystyle', media: 'all', 'data-turbolinks-track' => true %>
    
    <!-- jquery -->
    <%= javascript_include_tag 'jquery-2.1.4.min.js', 'data-turbolinks-track' => true %>
    <!-- Bootstrap -->
    <%= javascript_include_tag 'bootstrap.min.js', 'data-turbolinks-track' => true %>
    
    <!-- Plugin JavaScript -->
    <%= javascript_include_tag 'jquery.easing.min.js', 'data-turbolinks-track' => true %>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
<style>
.content {
    top: 40% !important;
}


#post-content {
    line-height: 37px;
    font-size: 20px;
    letter-spacing: 1px;
    font-family: 'Playfair Display', serif;
    margin-left: 0% !important;
    margin-top: -4%;
    font-weight: 600;
}
#wrapper-navbar {
    display: none;
}
#wrapper-header {
    display: none;
}

#main-container-footer{
    display:none;
}

.main-menu .navbar-right {
    display: none !important;
}


.container {
    position: initial;
}

.sticky-nav {
    background: transparent !important;
}

.navbar {
    position: initial;
}



:hov

.cls

element.style {
}
@media screen and (min-width: 768px)
bootstrap.min.s…….css?body=1:6
.jumbotron {
    padding-top: 48px;
    padding-bottom: 48px;
}
bootstrap.min.s…….css?body=1:6
.jumbotron {
    padding-top: 30px;
    padding-bottom: 30px;
    margin-bottom: 30px;
    color: inherit;
    background-color: #3F51B5;
}
bootstrap.min.s…….css?body=1:6
.text-center {
    text-align: center;
}

.jumbotron {
    height: 75vh;
    min-height: 100px;
    background: #3F51B5;

}


@media only screen and (min-device-width : 375px) and (max-device-width : 667px) { 
.navbar-header {
    display: none;
}
}

</style>
</head>
<body id="page-top" data-spy="scroll" data-target=".navbar">
<div id="main-wrapper">
    <!-- Page Preloader -->
    <div id="preloader">
        <div id="status">
            <div class="status-mes"></div>
        </div>
    </div>

    <header class="header">
        <!-- Navigation -->
        <nav class="navbar main-menu" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-main-collapse">
