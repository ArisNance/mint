                        <li class="hidden"><a href="#page-top"></a></li>
                        <li><a class="page-scroll" href="#section-intro">About</a></li>
                        <li><a class="page-scroll" href="#section-speaker">Speaker</a></li>
                        <li><a class="page-scroll" href="#section-ajenda">Agenda</a></li>
                        <li><a class="page-scroll" href="#section-pricing">Buy Ticket</a></li>
                        <li><a class="page-scroll" href="#section-partner">Partners</a></li>
                        <li><a class="page-scroll" href="#section-venue">Venue</a></li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container -->
        </nav>

        <!-- .nav -->

    </header>

    <div class="jumbotron text-center">
        <!--<img src="https://images.unsplash.com/photo-1492371451031-f0830e91b3d9?ixlib=rb-0.3.5&q=80&fm=jpg&crop=entropy&cs=tinysrgb&w=1917&fit=max&s=f7eeb6ee02532516580a5ffa1007fdd0">-->
        <div class="content">
            <div class="story-date"> | <%= @story.title %></div>
            <h1><%= @story.facebook_url %></h1>
            <p class="leads"><%= image_tag @story.header_image, style: "height: 70px;
            width: auto;" %></p>
            <br>
            <div class="story-date"><%= @story.description %></div>
            <button type="button" class="btn btn-default btn-lg"><%= link_to 'APPLY', @story.twitter_url, target: "_blank", id: "" %></button>
     
        </div>
    </div>
    <br>
    <br>
    <!-- .Jumbotron-->
<% if current_user %>
<%= link_to edit_story_path @story do %><i style="font-size: 15px; margin-left: 50%; color: #009688;" class="fa fa-pencil"><% end %></i>
<% end %>

    <section id="section-intro" class="section-wrapper about-story">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <p class="lead" id="post-content">
                        <%= simple_format(@story.content, id: "post-content") %>
                    </p>
                    <p class="lead"> </p>
                </div>
                <div class="col-md-4">
                    <address>
                        <strong>Where</strong><br>
                        <%= @story.facebook_url %>
                        <br>
                        <br>
                        <strong>Position</strong>
                        <br>
                        <%= @story.title %>
                        <br>
                        <br>
                        <strong>Apply Now</strong><br>
                        <%= link_to 'Go to Application', @story.twitter_url, target: "_blank", id: "" %>
                        <br>
                        <br>
                        <strong>Application End Date</strong><br>
                        <%= @story.end_time.strftime("%A, %b %d") %>
                        <br>
                        <br>
                        <strong>Share</strong>
                        <br>
                        <br>
                        <abbr title="Phone">
<!--share on twitter-->
   <%= link_to "http://twitter.com/home?status=Looking for a job? Apply to  #{@story.facebook_url} #{story_url(@story)}", target: "_blank" do %><i style="font-size: 30px; color: #009688;" class="fa fa-twitter" title="Share on Twitter"></i><% end %></abbr> 
                    </address>
                </div>
            </div>
        </div>
    </section>
